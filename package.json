{
  "name": "wearegenki-ui-monorepo",
  "private": true,
  "license": "Apache-2.0",
  "homepage": "https://ui.wearegenki.com",
  "scripts": {
    "setup": "yarn && lerna bootstrap",
    "update": "ncu && lerna exec --parallel -- ncu --reject /wearegenki/",
    "FIXME_start": "yarn dev",
    "FIXME_dev": "false",
    "clean": "lerna clean",
    "clean:dist": "lerna clean --yes && rm -rf node_modules",
    "publish:nightly": "lerna publish --canary=nightly --exact --npm-tag=next",
    "publish:beta": "lerna publish --canary=beta --exact --npm-tag=beta",
    "lint:css": "exit 0",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx,.vue,.marko packages docs test",
    "lint": "yarn lint:css && yarn lint:js",
    "jest": "jest --config test/jest.config.js --coverage",
    "test": "yarn lint && yarn jest",
    "test-ci": "yarn lint && yarn jest --runInBand",
    "FIXME_prepublish": "yarn build",
    "FIXME_build": "lerna run build",
    "debug:prefixes": "npx autoprefixer-info"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-stage-2": "7.0.0-beta.40",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "eslint": "4.18.2",
    "jest": "22.4.2",
    "jest-serializer-vue": "0.3.0",
    "lerna": "3.0.0-beta.0",
    "marko": "4.9.1",
    "postcss": "6.0.19",
    "puppeteer": "1.1.1",
    "stylelint": "9.1.1",
    "vue": "2.5.13",
    "vue-jest": "2.1.1",
    "vue-template-compiler": "2.5.13",
    "vue-test-utils": "1.0.0-beta.11"
  },
  "workspaces": [
    "packages/*",
    "docs/examples/*",
    "docs/website"
  ],
  "engines": {
    "node": "> 8.0.0"
  }
}
