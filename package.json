{
  "name": "wearegenki-ui-monorepo",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "setup": "yarn && lerna bootstrap --use-workspaces",
    "update": "ncu && lerna exec --parallel -- ncu --reject /wearegenki/",
    "FIXME_start": "npm run dev",
    "FIXME_dev": "false",
    "clean": "lerna clean",
    "clean:dist": "lerna clean --yes && rm -rf node_modules",
    "publish:canary": "lerna publish --canary --exact",
    "publish:next": "lerna publish --npm-tag=next",
    "lint:js": "eslint --ext .js,.vue,.marko packages test/unit/specs",
    "FIXME_lint": "lerna run lint",
    "FIXME_unit": "jest --config test/unit/jest.conf.js --coverage",
    "FIXME_test": "npm run lint && npm run unit",
    "FIXME_pretest": "npm run build",
    "FIXME_prepublish": "npm run build",
    "FIXME_build": "lerna run build",
    "debug:css": "node ./utils/debug-postcss.js",
    "debug:prefixes": "npx autoprefixer-info"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.37",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-stage-2": "^7.0.0-beta.37",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "eslint": "^4.15.0",
    "eslint-plugin-jest": "^21.6.1",
    "jest": "^22.0.6",
    "jest-serializer-vue": "^0.3.0",
    "lerna": "^2.7.0",
    "marko": "^4.7.4",
    "postcss": "^6.0.16",
    "postcss-debug": "^0.4.2",
    "postcss-load-config": "^1.2.0",
    "rollup": "^0.53.4",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-vue": "^3.0.0",
    "stylelint": "^8.4.0",
    "vue": "^2.5.13",
    "vue-jest": "^1.4.0",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "> 8.0.0"
  }
}
