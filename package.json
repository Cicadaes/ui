{
  "name": "@wearegenki/ui",
  "version": "0.1.1",
  "description": "CSS Grid based CSS library + Vue.js framework",
  "repository": "https://github.com/WeAreGenki/ui",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "style": "lib/index.css",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development rollup -c -w",
    "debug:css": "node ./debug-postcss.js",
    "test": "npm run lint && npm run unit",
    "pretest": "npm run build",
    "build": "rm -rf lib/* && npm run build:css && npm run build:js",
    "build:css": "postcss -o lib/index.css src/css/main.css",
    "build:js": "NODE_ENV=production rollup -c",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"src/**/*.vue\" \"src/**/*.css\"",
    "lint:js": "eslint --ext .js,.vue src test",
    "unit": "jest --config test/unit/jest.conf.js --coverage"
  },
  "dependencies": {
    "codemirror": "^5.33.0",
    "futurelink": "^0.2.1"
  },
  "peerDependencies": {
    "marko": "^4.7.4",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.36",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@babel/preset-stage-2": "^7.0.0-beta.36",
    "autoprefixer": "^7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "clean-css": "^4.1.9",
    "css-mqpacker": "^6.0.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-vue": "^3.13.0",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "marko": "^4.7.4",
    "postcss": "^6.0.14",
    "postcss-at-rules-variables": "^0.1.2",
    "postcss-calc": "^6.0.1",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.1",
    "postcss-conditionals": "^3.0.0-beta.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-debug": "^0.4.2",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-load-config": "^1.2.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "rollup": "^0.53.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-js": "^3.3.3",
    "vue-jest": "^1.4.0",
    "vue-template-compiler": "^2.5.13",
    "vue-test-utils": "^1.0.0-beta.9"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.10.0"
  },
  "browserslist": {
    "production": [
      "> 1% in AU",
      "> 1% in JP",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 ChromeAndroid versions",
      "last 2 Edge versions",
      "last 2 iOS versions",
      "last 2 Safari versions",
      "not Explorer <= 11",
      "not ExplorerMobile <= 11"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
