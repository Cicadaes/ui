{
  "name": "wearegenki-ui-monorepo",
  "private": true,
  "license": "Apache-2.0",
  "homepage": "https://ui.wearegenki.com",
  "scripts": {
    "setup": "yarn && lerna bootstrap --use-workspaces",
    "update": "ncu && lerna exec --parallel -- ncu --reject /wearegenki/",
    "FIXME_start": "yarn dev",
    "FIXME_dev": "false",
    "clean": "lerna clean",
    "clean:dist": "lerna clean --yes && rm -rf node_modules",
    "publish:canary": "lerna publish --canary --exact",
    "publish:next": "lerna publish --npm-tag=next",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.jsx,.vue,.marko packages docs test utils",
    "lint": "lerna run lint",
    "unit": "jest --config test/jest.config.js --coverage",
    "test": "yarn lint && yarn unit",
    "FIXME__test-ci": "yarn lint && yarn unit --runInBand",
    "test-ci": "yarn unit --runInBand",
    "FIXME_prepublish": "yarn build",
    "FIXME_build": "lerna run build",
    "debug:css": "node ./utils/debug-postcss.js",
    "debug:prefixes": "npx autoprefixer-info"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-stage-2": "^7.0.0-beta.40",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.2",
    "eslint": "4.18.2",
    "jest": "22.4.2",
    "jest-serializer-vue": "^0.3.0",
    "lerna": "2.9.0",
    "marko": "^4.9.1",
    "postcss": "6.0.19",
    "postcss-debug": "0.4.2",
    "postcss-load-config": "1.2.0",
    "puppeteer": "1.1.1",
    "stylelint": "9.1.1",
    "vue-jest": "^2.1.1",
    "vue-template-compiler": "^2.5.13"
  },
  "workspaces": [
    "packages/*",
    "docs/examples/*",
    "docs/website"
  ],
  "engines": {
    "node": "> 8.0.0"
  }
}
