// cSpell Settings
{
    // Version of the setting file.  Always 0.1
    "version": "0.1",
    // language - current active spelling language
    "language": "en-GB",
    // words - list of words to be always considered correct
    "words": [
        "antialiased",
        "browserslist",
        "buble",
        "checkbox",
        "codemirror",
        "compat",
        "dialog",
        "dropdown",
        "flexbox",
        "futurelink",
        "jsnext",
        "linting",
        "minmax",
        "mixins",
        "mqpacker",
        "noopener",
        "ontouchstart",
        "plusplus",
        "polyfill",
        "preload",
        "presets",
        "readonly",
        "rgba",
        "Segoe",
        "SVGs",
        "transpiled",
        "typeof",
        "upsert",
        "visibilitychange",
        "vuex",
        "xlink",
        "xsite"
    ],
    // flagWords - list of words to be always considered incorrect
    // This is useful for offensive words and common spelling errors.
    // For example "hte" should be "the"
    "flagWords": [
        "hte"
    ]
}
