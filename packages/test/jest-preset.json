{
  "XX__testMatch": ["**/?(*.)(spec|test).js"],

  "testPathIgnorePatterns": [
    "/coverage/",
    "/dist/",
    "/node_modules/",
    "/\\.vscode/",
    "/\\.cache/",
    "/\\.eslintcache/",
    "/\\.git/"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "marko",
    "mjs",
    "node",
    "vue"
  ],
  "transform": {
    "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff2?)$": "<rootDir>/node_modules/@wearegenki/test/lib/null-transform.js",
    "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
    "^.+\\.marko$": "<rootDir>/node_modules/@wearegenki/test/lib/marko/transform.js",
    "^.+\\.vue$": "<rootDir>/node_modules/vue-jest"
  },
"  snapshotSerializers": [
    "<rootDir>/node_modules/jest-serializer-vue"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,marko,mjs,vue}",
    "!**/\\.*rc.js",
    "!**/*\\.{config,marko}\\.js",
    "!**/*\\.{test,spec}\\.js",
    "!**/__mocks__/**",
    "!**/build/**",
    "!**/config/**",
    "!**/dist/**",
    "!**/fixtures/**",
    "!**/node_modules/**",
    "!**/public/**",
    "!**/test/**",
    "!**/types/**",
    "!**/\\.cache/**",
    "!**/\\.esm-cache/**",
    "!**/\\.git/**",
    "!**/\\.vscode/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "coverageDirectory": "<rootDir>/test/coverage",
  "coverageReporters": ["json", "text"]
}
