{
  "XX__globalSetup": "<rootDir>/node_modules/@wearegenki/test/lib/puppeteer/setup.js",
  "XX__globalTeardown": "<rootDir>/node_modules/@wearegenki/test/lib/puppeteer/teardown.js",
  "XX__testEnvironment": "<rootDir>/node_modules/@wearegenki/test/lib/puppeteer/environment.js",

  "testMatch": ["**/?(*.)(spec|test).js"],
  "testPathIgnorePatterns": [
    "/coverage/",
    "/dist/",
    "/node_modules/",
    "/\\.vscode/",
    "/\\.cache/",
    "/\\.eslintcache/",
    "/\\.git/"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "marko",
    "mjs",
    "node",
    "vue"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,marko,mjs,vue}",
    "!**/\\.*rc.js",
    "!**/*\\.{config,marko}\\.js",
    "!**/*\\.{test,spec}\\.js",
    "!**/__mocks__/**",
    "!**/build/**",
    "!**/config/**",
    "!**/dist/**",
    "!**/fixtures/**",
    "!**/node_modules/**",
    "!**/public/**",
    "!**/test/**",
    "!**/types/**",
    "!**/\\.cache/**",
    "!**/\\.esm-cache/**",
    "!**/\\.git/**",
    "!**/\\.vscode/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "coverageDirectory": "<rootDir>/test/coverage",
  "coverageReporters": ["json", "text"]
}
