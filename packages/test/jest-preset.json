{
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/",
    "/manual-tests/",
    "/mocks/",
    "/\\.vscode/",
    "/\\.cache/",
    "/coverage/"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "marko",
    "node",
    "vue"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,marko,ts,tsx,vue}",
    "!**/\\.*rc.js",
    "!**/*.marko.js",
    "!**/*.min.js",
    "!**/*.{test,spec}.js",
    "!**/__mocks__/**",
    "!**/__tests__/**",
    "!**/build/**",
    "!**/config/**",
    "!**/dist/**",
    "!**/node_modules/**",
    "!**/public/**",
    "!**/test/**",
    "!**/tmp/**",
    "!**/types/**",
    "!**/vendor/**",
    "!**/.cache/**",
    "!**/.esm-cache/**",
    "!**/.git/**",
    "!**/.vscode/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "coverageReporters": ["text"]
}
