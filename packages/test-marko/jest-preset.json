{
  "transform": {
    "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff2?)$": "@wearegenki/test/lib/null-transform.js",
    "^.+\\.js$": "babel-jest",
    "^.+\\.marko$": "@wearegenki/test-marko/lib/transform.js"
  },
  "testPathIgnorePatterns": [
    "/coverage/",
    "/dist/",
    "/node_modules/",
    "/\\.vscode/",
    "/\\.cache/",
    "/\\.eslintcache/",
    "/\\.git/"
  ],
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "marko",
    "mjs"
  ],
  "collectCoverageFrom": [
    "**/*.{js,jsx,marko,mjs}",
    "!**/\\.*rc.js",
    "!**/*\\.{config,marko}\\.js",
    "!**/*\\.{test,spec}\\.js",
    "!**/__mocks__/**",
    "!**/build/**",
    "!**/config/**",
    "!**/dist/**",
    "!**/fixtures/**",
    "!**/node_modules/**",
    "!**/public/**",
    "!**/test/**",
    "!**/types/**",
    "!**/\\.cache/**",
    "!**/\\.esm-cache/**",
    "!**/\\.git/**",
    "!**/\\.vscode/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "coverageDirectory": "<rootDir>/test/coverage"
}
