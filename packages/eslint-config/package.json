{
  "name": "@wearegenki/eslint-config",
  "version": "0.4.1",
  "description": "ESLint config presets for use in We Are Genki projects.",
  "keywords": [
    "eslint",
    "vue",
    "marko",
    "markojs",
    "wearegenki"
  ],
  "repository": "https://github.com/WeAreGenki/ui/tree/master/packages/eslint-config",
  "bugs": {
    "url": "https://github.com/WeAreGenki/ui/issues"
  },
  "author": "Max Milton <max@wearegenki.com>",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "build": "exit 0",
    "prepare": "npm run build",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"src/**/*.vue\" \"src/**/*.css\"",
    "lint:js": "eslint --ext .js,.vue src test",
    "test": "exit 0"
  },
  "peerDependencies": {
    "babel-eslint": ">7.0.0",
    "eslint": "^4.14.0"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.2.0"
  },
  "engines": {
    "node": "> 8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
