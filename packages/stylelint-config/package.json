{
  "name": "@wearegenki/stylelint-config",
  "version": "0.4.1",
  "description": "Stylelint config preset for use in We Are Genki projects.",
  "keywords": [
    "css",
    "cssgrid",
    "design-system",
    "stylelint",
    "wearegenki"
  ],
  "repository": "https://github.com/WeAreGenki/ui/tree/master/packages/stylelint-config",
  "bugs": {
    "url": "https://github.com/WeAreGenki/ui/issues"
  },
  "author": "Max Milton <max@wearegenki.com>",
  "license": "Apache-2.0",
  "main": "index.js",
  "scripts": {
    "build": "exit 0",
    "prepare": "npm run build",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"src/**/*.vue\" \"src/**/*.css\"",
    "lint:js": "eslint --ext .js,.vue src test",
    "test": "exit 0"
  },
  "dependencies": {
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0"
  },
  "peerDependencies": {
    "stylelint": "^8.4.0"
  },
  "engines": {
    "node": "> 8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
